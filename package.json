{
  "name": "messaging-microservice",
  "version": "1.0.0",
  "description": "Healthcare messaging microservice with Sendbird integration",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "db:migrate": "ts-node src/scripts/migrate.ts",
    "setup:system-user": "ts-node src/scripts/createSystemUser.ts",
    "setup:compliance": "ts-node src/scripts/setupCompliance.ts",
    "update:test-user-admin": "ts-node src/scripts/updateTestUserToAdmin.ts",
    "add:unique-constraints": "ts-node src/scripts/addUniqueConstraints.ts",
    "quick:fix": "ts-node src/scripts/quickFix.ts",
    "diagnose:db": "ts-node src/scripts/diagnoseDb.ts",
    "test:sendbird": "ts-node src/scripts/testSendbird.ts",
    "setup:compliance-groups": "ts-node src/scripts/setupComplianceGroups.ts",
    "add:consent-attachments": "ts-node src/scripts/addConsentAttachments.ts",
    "sync:sendbird-users": "ts-node src/scripts/syncSendbirdUsers.ts",
    "create:test-users": "ts-node src/scripts/createTestUsers.ts",
    "create:test-patients": "ts-node src/scripts/createTestPatients.ts",
    "check:consent": "ts-node src/scripts/checkAndFixConsent.ts",
    "create:consent": "ts-node src/scripts/createConsent.ts",
    "render-build": "./scripts/build.sh"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "pg": "^8.11.3",
    "sendbird": "^3.1.33",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "express-validator": "^7.0.1",
    "winston": "^3.11.0",
    "axios": "^1.6.2",
    "multer": "^2.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/uuid": "^9.0.7",
    "typescript": "^5.3.2",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.1"
  }
} 